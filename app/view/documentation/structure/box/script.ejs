<script>

  function renderDefault() {
    let a_box = document.getElementById('a-box');
    let b_box = document.getElementById('b-box');
    for (let i = 1; i <= 12; i++) {
      let a_section = lib.element.create("div", { class: "box a1 container" });
      let b_section = lib.element.create("div", { class: "box a1 container" });

      for (let j = 1; j <= i; j++) {
        a_section.append(lib.element.create("div", { class: `mobile-box a${i} lucida-grande em08 bold padding-1 border-st center` }, `A${i}`));
        a_box.append(a_section);

        b_box.append(lib.element.create("div", { class: `mobile-box b${i} lucida-grande em08 bold padding-1 border-st center` }, `B${i}`));
        b_box.append(b_section);
      };
    };
  };

  renderDefault();
</script>